{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": null,
  "buildCommand": "echo \"Deploy static homepage directory\"",
  "outputDirectory": "website",
  "rewrites": [
    {
      "source": "/health",
      "destination": "https://api.atlasmasa.com/health"
    },
    {
      "source": "/v1/:path*",
      "destination": "https://api.atlasmasa.com/v1/:path*"
    }
  ]
}
